{
  "name": "tsx-to-pdf",
  "version": "1.0.0",
  "description": "Convert TSX files to PDF with proper formatting",
  "type": "module",
  "main": "ConverterService/index.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "start": "node start-all.js",
    "start-legacy": "node run.js",
    "convert": "node ConverterService/cli.js",
    "setup-dev-server": "node setup-dev-server.js",
    "build-frontend": "node ConverterService/build-frontend.js",
    "start-server": "node ConverterService/server.js",
    "start-ui": "npm run build-frontend && npm run start-server",
    "start-dev-server": "cd RenderingServer && npm run dev",
    "dev": "node watch-and-build.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "lucide-react": "^0.476.0",
    "multer": "^1.4.5-lts.1",
    "pdf-lib": "^1.17.1",
    "puppeteer": "^24.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "^0.174.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@svgr/webpack": "^8.1.0",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "@types/three": "^0.174.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.10.0",
    "html-webpack-plugin": "^5.6.0",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  }
